{
  "name": "my-bass-app",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": "",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "start": "webpack-dashboard -m -- webpack-dev-server --config config/webpack.config.js --open --env.local",
    "start-remote": "webpack-dashboard -m -- webpack-dev-server --config config/webpack.config.js --https",
    "cors-enable": "sfdx force:mdapi:deploy -d ./config/sfdc-cors-enable -w 10000",
    "cors-disable": "sfdx force:mdapi:deploy -d ./config/sfdc-cors-disable -w 10000",
    "clean": "rimraf dist",
    "compile-dev": "npm run clean && webpack --config config/webpack.config.js --env.build --env.sourceMap",
    "compile-prod": "npm run clean && webpack --config config/webpack.config.js --env.build --env.sourceMap",
    "new-scratch-org": "sfdx force:org:create -f config/project-scratch-def.json -a scratch",
    "make-scratch-default": "sfdx force:config:set defaultusername=scratch",
    "make-dev-default": "sfdx force:config:set defaultusername=dev",
    "make-prod-default": "sfdx force:config:set defaultusername=prod",
    "bundle-app-dev": "npm run compile-dev && npm run copy-bundle",
    "bundle-app-prod": "webpack run compile-prod && npm run copy-bundle",
    "copy-bundle": "cp ./dist/app.js ./force-app/main/default/staticresources/App.resource",
    "package-sfdc": "sfdx force:source:convert -d ./dist/sfdc-package",
    "build-dev": "npm run bundle-app-dev && npm run package-sfdc",
    "build-prod": "npm run bundle-app-prod && npm run package-sfdc",
    "push-dev": "sfdx force:mdapi:deploy -u dev -d ./dist/sfdc-package -w 10000",
    "push-scratch": "sfdx force:source:push -u scratch",
    "push-prod": "sfdx force:mdapi:deploy -u production -d ./dist/sfdc-package -w 10000",
    "deploy-dev": "npm run build-dev && npm run push-dev",
    "deploy-scratch": "npm run build-dev && npm run push-scratch",
    "deploy-prod": "npm run build-prod && npm run push-prod",
    "generate-ts-force": "ts-force-gen -j ./config/ts-force-config.json",
    "test": "jest --no-cache --config jest.json",
    "test:update": "jest --no-cache --config jest.json -u",
    "reinstall": "rm -rf node_modules && npm install",
    "reinstall:win": "rd /s /q node_modules && npm"
  },
  "dependencies": {
    "antd": "^2.13.2",
    "babel-jest": "^21.0.2",
    "babel-polyfill": "^6.26.0",
    "csstips": "^0.2.0",
    "immutability-helper": "^2.4.0",
    "lodash": "^4.17.4",
    "query-string": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "ts-force": "^1.0.29",
    "tslib": "^1.6.1",
    "typestyle": "^1.3.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.8",
    "@types/jest": "^21.1.0",
    "@types/node": "^7.0.14",
    "@types/react": "^15.0.6",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack": "^2.2.15",
    "@types/webpack-dev-server": "^1.12.6",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.5.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^19.0.14",
    "tslint": "^5.1.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.5.2",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.3.0"
  }
}
